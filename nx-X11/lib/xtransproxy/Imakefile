NULL =

#define IncSubdir nx-X11

all::

DEFINES = $(FAILDEFINES)

# Since xtrans source is #include'd into software that uses it, we install
# the .c files as if they were headers.
HEADERS =			\
	Xtransproxy.h		\
	Xtransproxyint.h		\
	Xtranssockproxy.c	\
	transport.c		\
	$(NULL)

DependTarget()
BuildIncludes($(HEADERS),IncSubdir/XproxyTrans,../..)
